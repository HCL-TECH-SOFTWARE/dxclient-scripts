<request
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="PortalConfig_8.0.0.xsd"
    type="update">

    <!-- Sample for deleting the page with unique name 'wps.content.root.WelcomePage'.
    
         NOTE: This sample expects that a page with the unique name 'wps.content.root.WelcomePage' exists.
               For example the DeployPortlet.xml sample creates such a page.
     -->
    <portal action="locate">

        <content-node action="delete" uniquename="wps.content.root.WelcomePage">
        </content-node>

    </portal>
</request>
